[32m+[m[32mAS_metadata <- create.MethylPos(AS_seq, AS, AS_methyl)[m
[32m+[m
[32m+[m[32mAS_metadata_filename <- paste0(save_folder, "AS_metadata_", bp_ext, "bp.csv")[m
[32m+[m[32mwrite.csv(AS_metadata, AS_metadata_filename)[m
[32m+[m
