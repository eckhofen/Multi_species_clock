[32m+[m[32mecho "AC"[m
[32m+[m[32mfor (( i=0; i<${#seq_1000bp[@]}; i++ ));[m[41m [m
[32m+[m[32mdo[m[41m [m
[32m+[m[32m    bowtie2 $bowtie2_args -x ${path_raw}AC/bowtie2-index/AC_bowtie2 -f -U ${path_sequences}${seq_1000bp[$i]}$suffix -S ${path_results}AC_${seq_1000bp[$i]}_bt2_.sam -N 1[m
[32m+[m[32mdone[m
[32m+[m
