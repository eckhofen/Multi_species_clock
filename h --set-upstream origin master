[32m+[m[32moverlap_seqs_bt2_AC <- find.Overlap(AC_AC_1000_bt2,[m
[32m+[m[32m                                    AC_AS_1000_bt2,[m
[32m+[m[32m                                    AC_EH_1000_bt2,[m[41m [m
[32m+[m[32m                                    AC_JM_1000_bt2,[m
[32m+[m[32m                                    AC_ZF_1000_bt2[m
[32m+[m[32m                                    )[m
