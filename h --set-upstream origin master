[32m+[m[32m    "        file1=\"${file}_1.1.1.fq.gz\"\n",[m
[32m+[m[32m    "        file2=\"${file}_2.2.2.fq.gz\"\n",[m
[32m+[m[32m    "#echo ${PREFIX}\n",[m
[32m+[m[32m    "#echo ${file1}\n",[m
[32m+[m[32m    "#echo ${file2}\n",[m
[32m+[m[32m    "\n",[m
