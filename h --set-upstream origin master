[32m+[m[32mpath_sequences=/workspace/cfngle/results-data/sequences/[m
[32m+[m
[32m+[m[32m# filenames[m
[32m+[m[32mseq_100bp=("AC_CpG_100bp.fasta" "AS_CpG_100bp.fasta" "EH_CpG_100bp.fasta")[m
[32m+[m
