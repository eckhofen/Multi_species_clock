[32m+[m[32m# AS[m
[32m+[m[32mcd-hit-est-2d -i ${path_sequences}${seq_500bp[1]} -i2 ${path_sequences}${seq_500bp[2]} -o ${path_results}cd-hit/AS_EH_500_cd-hit${cdhit_nmsfx}.fasta $cdhit_opt[m
