[32m+[m[32m    "                --genome ${GENOME} \\\n",[m
[32m+[m[32m    "                $file \\\n",[m
[32m+[m[32m    "                --o ${OUT}/${PREFIX} \\\n",[m
[32m+[m[32m    "                --multicore 5 \\\n",[m
[32m+[m[32m    "                --non_directional \\\n",[m
[32m+[m[32m    "                --local\"\n",[m
[32m+[m[32m    "       # echo \"${OUT}${PREFIX}\"\n",[m
[32m+[m[32m    "       echo $COMMAND\n",[m
[32m+[m[32m    "\n",[m
