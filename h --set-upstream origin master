[32m+[m[32m  gsub("AC_", "Chr", .)[m
[32m+[m[32mAC_meth_data_test <- gsub("X", "Chr", colnames(AC_meth_data))[m
[32m+[m[32mtable(meth_sites_names_tmp %in% AC_meth_data_test)[m
