[32m+[m[32m    "    samtools view -Sb \"$samfile\" > \"$bamfile\"\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "    echo \"Converted $samfile to $bamfile\"\n",[m
[32m+[m[32m    "done"[m
[32m+[m[32m   ][m
[32m+[m[32m  },[m
[32m+[m[32m  {[m
