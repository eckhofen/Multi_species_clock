[32m+[m[32m)[m
[32m+[m[32mcolnames(df_methyl_data_bt2) <- c("method","species", "CpGs","CpGs_filtered","seqs_overlap","seqs_filtered", "CpGs_aligned", "seqs_aligned")[m
[32m+[m
[32m+[m[32mdf_methyl_data_mini <- data.frame([m
[32m+[m[32m  c("mini"),[m
