[32m+[m[32m##AS[m
[32m+[m[32mPCA_AS <- prcomp(AS_meth_values,scale = TRUE)[m
[32m+[m[32mPCA_values_AS <- as.data.frame(PCA_AS$x)[m
[32m+[m[32mPCA_values_AS$species <- "AS"[m
[32m+[m
[32m+[m[32m##EH[m
[32m+[m[32mPCA_EH <- prcomp(EH_meth_values,scale = TRUE)[m
