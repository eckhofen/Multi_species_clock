[32m+[m[32mAC_all_bt2_genes_names <- unique(mcols(AC_all_bt2_genes)$Name)[m
[32m+[m
[32m+[m
[32m+[m[32munique(mcols(AC_annotations)$type)[m
[32m+[m
[32m+[m
