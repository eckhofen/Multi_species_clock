[32m+[m[32m    "                --genome ${GENOME} \\\n",[m
[32m+[m[32m    "                -1 ${file1} \\\n",[m
[32m+[m[32m    "                -2 ${file2} \\\n",[m
[32m+[m[32m    "                --o ${OUT} \\\n",[m
[32m+[m[32m    "                --non_directional\"\n",[m
