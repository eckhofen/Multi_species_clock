[32m+[m[32massign("AC_meth_data", get(xx))[m
[32m+[m[32mAC_meth_data <- as.data.frame(AC_meth_data)[m
[32m+[m[32m# tail(colnames(AC_meth_data))[m
[32m+[m[32mAC_age <- AC_meth_data$age[m
[32m+[m
