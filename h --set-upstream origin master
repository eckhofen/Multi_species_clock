[32m+[m[32mseq_200bp=("AC_CpG_200bp" "AS_CpG_200bp" "EH_CpG_200bp")[m
[32m+[m[32mseq_500bp=("AC_CpG_500bp" "AS_CpG_500bp" "EH_CpG_500bp")[m
