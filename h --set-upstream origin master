[32m+[m[32mJM_meth_data <- load("/workspace/cfngle/raw-data/JM/zzz-methyldata/00_JM_methyldata_243285_CpGs.Rdata")[m
[32m+[m[32mJM_meth_data <- JM_24_methyl_data[m
[32m+[m
